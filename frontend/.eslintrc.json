{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "react/jsx-sort-props": "off",
    "jsx-a11y/label-has-associated-control": "off",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ]
  }
}
